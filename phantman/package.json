{
  "name": "b2c-mobile",
  "description": "",
  "author": "GZNB frontend team",
  "scripts": {
    "generator": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "dev": "cross-env NODE_ENV=development node ./build/server.development.js",
    "mock": "nodemon --watch ./mock build/mock-service.js",
    "build:Prd": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "lint": "eslint --ext .js,.vue app",
    "postbuild": "cp -r dist ../B2CMobile-deploy && cp -r public ../B2CMobile-deploy && cp -r build ../B2CMobile-deploy && cp -r config ../B2CMobile-deploy",
    "build:Test": "rimraf dist && npm run build:client-test && npm run build:server-test",
    "build:client-test": "cross-env NODE_ENV=test webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server-test": "cross-env NODE_ENV=test webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build:Pre": "rimraf dist && npm run build:client-pre && npm run build:server-pre",
    "build:client-pre": "cross-env NODE_ENV=pre webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server-pre": "cross-env NODE_ENV=pre webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.0"
  },
